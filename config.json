{
  /* The ID of the node instance */
  "nodeId" : 1,

  /* The port to listen on */
  "port" : 4500,   /* 443 */

  /* Enable or disable authentication */
  "authentication" : true,

  /* Session TTL in minutes (default: 15 minutes) */
  "sessionTTL" : 15,

  /* Redis configuration */
  "redis" : {
    "host" : "localhost",
    "port" : 6379
  },

  /* SSL configuration */
  "ssl" : {
    "enabled" : false,
    "key" : "ssl/symple.key",
    "cert" : "ssl/symple.crt"
  }
}
